{
  "name": "quill-video-embed",
  "version": "0.0.4",
  "description": "A quill editor module for video embeds.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && babel src --presets=es2015 --out-dir dist",
    "watch": "watch 'yarn run build' './src' --wait=0.3",
    "prepublish": "yarn build"
  },
  "keywords": [
    "quill",
    "editor",
    "video",
    "embed"
  ],
  "author": "Adam Miller",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "get-video-id": "^3.1.4"
  }
}
